<div id="<%= dom_id photo %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Title:</strong>
    <%= photo.title %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= photo.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Taken at:</strong>
    <%= photo.taken_at %>
  </div>
</div>
